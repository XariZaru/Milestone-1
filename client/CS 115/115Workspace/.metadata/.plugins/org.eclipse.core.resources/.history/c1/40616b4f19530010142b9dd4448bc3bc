import java.util.ArrayList;

// The manager runs the show
public class MusicManager extends MusicManagerParent
{
	public MusicManager()
	{
		super();
	}
	
	// Write out the sorted music list to the index file
	protected void writeMusicIndex()
	{

		indexFile.open(INDEX_FILE_NAME);
			for (int i=0;i<musicList.getBuckets().size();i++)
			{
				for (int m=0;m<musicList.getBuckets().get(i).getItems().size();m++)
				{
					musicList.getBuckets().get(i).getItems().get(m);
				}
			}
		indexFile.close();
	}
}
